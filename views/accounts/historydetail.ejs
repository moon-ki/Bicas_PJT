<% include ../header.ejs %>

<% include ./sidebar.ejs %>
    
    
    
    <br/>   
    

    
      <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <div class="login-panel panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">트랜잭션 세부정보</h3>
                </div>
                <div class="panel-body">
                    <form role="form" action="" id="songguem_form" method="post">
                        <fieldset>

                            <h2 id="instructor"></h2>
                        
                            <div class="form-group">
                                <li>Hash</li>
                                <input class="form-control" placeholder="" id="user_id" name="user_id" readonly type="text" value="<%= transact.t_hash.replace(/\"/gi, ""); %>" required="">
                            </div>
                            <div class="form-group">
                                <li>From</li>
                                <input class="form-control" placeholder="" id="user_name" name="user_name" readonly type="text" value="<%= transact.from %>" required="">
                            </div>
                            <div class="form-group">
                                <li>To</li>
                                <input class="form-control" placeholder="" id="blockchainid" name="blockchainid" readonly type="text" value="<%= transact.to %>" required="">
                            </div>
                            <div class="form-group">
                                <li>Ether</li>
                                <input class="form-control" placeholder="" id="blockchainid" name="blockchainid" readonly type="text" value="<%= transact.ether %>" required="">
                            </div>
                            <div class="form-group">
                                <li>Gas</li>
                                <input class="form-control" placeholder="" id="blockchainid" name="blockchainid" readonly type="text" value="<%= transact.gas %>" required="">
                            </div>
                            <div class="form-group">
                                <li>GasPrice</li>
                                <input class="form-control" placeholder="" id="blockchainid" name="blockchainid" readonly type="text" value="<%= transact.gasPrice.replace(/\"/gi, ""); %>" required="">
                            </div>
                            <div class="form-group">
                                <li>r</li>
                                <input class="form-control" placeholder="" id="blockchainid" name="blockchainid" readonly type="text" value="<%= transact.r.replace(/\"/gi, "");%>" required="">
                            </div>
                            <div class="form-group">
                                <li>s</li>
                                <input class="form-control" placeholder="" id="blockchainid" name="blockchainid" readonly type="text" value="<%= transact.s.replace(/\"/gi, ""); %>" required="">
                            </div>
                            <div class="form-group">
                                <li>v</li>
                                <input class="form-control" placeholder="" id="blockchainid" name="blockchainid" readonly type="text" value="<%= transact.v.replace(/\"/gi, ""); %>" required="">
                            </div>
                            <div class="form-group">
                                <li>Time</li>
                                <input class="form-control" placeholder="" id="blockchainid" name="blockchainid" readonly type="text" value="<%=transact.getDate.year%>.<%=transact.getDate.month%>.<%=transact.getDate.day %>. <%=transact.getDate.hour%>:<%=transact.getDate.minute%>:<%=transact.getDate.second%>"%>" required="">
                            </div>
                            <a href="/accounts/history" class="btn btn-default">목록으로</a>
                        </fieldset>
                    </form>
                    <h2 id="instructor"></h2>
                </div>
            </div>
        </div>
</div>
      

