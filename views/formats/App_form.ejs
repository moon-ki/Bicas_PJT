<script>
	//증명서 명 셋팅, 현재날짜 셋팅, 	
    function pad(n, width) {
        n = n.toString();//string 변환
        return n.length >= width ? n : new Array(width - n.length + 1).join('0') + n;
    }   

		var date = new Date();
		$("#title1").val('<%=form_name%>');
		$('#applyDate').val(
				date.getFullYear()+
				pad(date.getMonth(),2)+
				pad(date.getDate(),2)
		);
</script>
